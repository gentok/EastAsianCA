@article{ WOS:000272175900011,
Author = {Peyrouse, Sebastien},
Title = {Is there any unity to the Trans-Caspian Region? The economic relations
   between Central Asia and the Caucasus},
Journal = {ASIA EUROPE JOURNAL},
Year = {2009},
Volume = {7},
Number = {3-4},
Pages = {543-557},
Month = {DEC},
Abstract = {Since the disappearance of the Soviet Union, the three states of South
   Caucasus or Trans-Caucasus (Azerbaijan, Armenia, and Georgia) and the
   five of Central Asia (Kazakhstan, Kyrgyzstan, Uzbekistan, Tajikistan,
   and Turkmenistan) have played up their role as intermediaries between
   Europe and Asia and have tried to constitute the Caspian as an entity.
   This new Trans-Caspian road obeys political and economic logics as the
   states of the South Caucasus along with those of Central Asia are
   interested in bypassing Russia and developing their bilateral relations
   to acquire new markets in Europe and in Asia. Despite the existence of
   sometimes divergent, sometimes convergent interests, and the
   non-negligible political and economic risks, none of the states of the
   South Caucasus, nor those of Central Asia, want to undermine the dynamic
   of Trans-Caspian rapprochement.},
