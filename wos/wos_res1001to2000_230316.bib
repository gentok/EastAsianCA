@article{ WOS:000261171700001,
Author = {Roscini, Marco},
Title = {Something Old, Something New: The 2006 Semipalatinsk Treaty on a Nuclear
   Weapon-Free Zone in Central Asia},
Journal = {CHINESE JOURNAL OF INTERNATIONAL LAW},
Year = {2008},
Volume = {7},
Number = {3},
Pages = {593-624},
Month = {NOV},
Abstract = {The present article analyses the provisions of the 2006 Semipalatinsk
   Treaty establishing a nuclear weapon-free zone in Central Asia, explores
   their different nature and compares them with those contained in the
   Treaties of Tlatelolco, Rarotonga, Bangkok and Pelindaba. The
   fundamental question to be answered is whether the Semipalatinsk Treaty
   will effectively contribute to the non-proliferation of nuclear weapons.
   The article concludes that the treaty contains lights and shadows:
   although some positive innovations have been included in the final text,
   there are loopholes that might weaken the denuclearization regime. The
   ``Great Game{''} in Central Asia and the tight relations of the regional
   States with the Russian Federation might also hamper the efforts to
   obtain the support of the other nuclear powers.},
