@article{ WOS:000658894000143,
Author = {Azizov, Sherzod and Karimjonova, Munavvar and Makhkamov, Zokirjon},
Title = {Some Methods of Establishment of a Modern Customs Audit Mechanism in
   Uzbekistan},
Journal = {REVISTA GEINTEC-GESTAO INOVACAO E TECNOLOGIAS},
Year = {2021},
Volume = {11},
Number = {2},
Pages = {1778-1794},
Month = {APR-JUN},
Abstract = {The article highlights the ongoing reforms in Uzbekistan in connection
   with the creation of a modern customs audit mechanism, the measures
   taken, its relevance to the general audit and accounting, differences
   and similarities. It is also necessary to thoroughly analyze the process
   of customs audit, the disclosure and implementation of its content,
   accounting documents and financial statements, the importance of the
