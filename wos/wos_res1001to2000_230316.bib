@article{ WOS:000483808300010,
Author = {Morrison, Alexander},
Title = {`The extraordinary successes which the Russians have achieved'-the
   Conquest of Central Asia in Callwell's Small Wars},
Journal = {SMALL WARS AND INSURGENCIES},
Year = {2019},
Volume = {30},
Number = {4-5, SI},
Pages = {913-936},
Month = {JUL 29},
Abstract = {Charles Callwell's Small Wars (1896, 1899, 1906) is widely considered
   both an ur-text for modern counter-insurgency studies, and a primer for
   the racialized late-Victorian approach to war against `savages': either
   way it is usually only considered within a British context. Alongside
   the numerous examples Callwell used from British colonial campaigns, he
   frequently referred to those of other European powers - notably the
   Russian conquest of Central Asia. This article will seek to analyse
   Callwell's views of Russian colonial warfare, establish the sources on
