@article{ WOS:000611609000003,
Author = {Sabirova, Dina A. and Abisheva, Saule D. and Polyak, Zinaida N. and
   Polyak, Dina M. and Kubdasheva, Karligash B.},
Title = {Transformation of Family Norms in Modern Literature of Kazakhstan},
