@article{ WOS:000412362700004,
Author = {Davalos, Jorge and Karymshakov, Kamalbek and Sulaimanova, Burulcha and
   Abdieva, Raziiakhan},
Title = {Remittances and labor supply of the left-behind youth: Evidence from
   Kyrgyzstan},
Journal = {ASIAN AND PACIFIC MIGRATION JOURNAL},
Year = {2017},
Volume = {26},
Number = {3},
Pages = {352-380},
Month = {SEP},
Abstract = {This paper examines whether remittances from international migration
   impact on the occupational choice of left-behind youth in Kyrgyzstan.
   Labor supply is analyzed both at the extensive and intensive margins
   using cross-sectional data for 2011. To overcome endogeneity concerns,
   an instrumental variable approach was implemented. Findings demonstrate
   that migration, rather than remittances, pushes the left-behind youth to
   become unpaid family workers. This is explained by the substitution
   effect as the youth left behind are called upon to replace the migrant
   labor. Moreover, this effect is heterogeneous - female youth are more
   inclined to becoming unpaid family workers both at the extensive and
   intensive margins.},
