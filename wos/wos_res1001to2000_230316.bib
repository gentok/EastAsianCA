@article{ WOS:000701501400003,
Author = {Tang, Shiping},
Title = {Regionalism in the Shadow of Extraregional Great Powers: A Game
   Theoretical Explanation of Central Asia and Beyond(dagger)},
Journal = {CHINESE JOURNAL OF INTERNATIONAL POLITICS},
Year = {2021},
Volume = {14},
