@article{ WOS:000250419300003,
Author = {Rosenberger, Nancy},
Title = {Patriotic appetites and gnawing hungers: Food and the paradox of
   nation-building in Uzbekistan},
Journal = {ETHNOS},
Year = {2007},
Volume = {72},
Number = {3},
Pages = {339-360},
Abstract = {Nations reverberate with the conundrums of unity and difference. Studies
   of food provide an effective way to understand this paradox as food
   indexes both rituals of the nation as national cuisine yet links with
   myriad and variable performances as it is grown, cooked and eaten.
   National foods enhance ethnic nationality in newly independent,
   increasingly authoritarian Uzbekistan where the focus is on national
   cuisine in part because of deprivation, rather than abundance. National
   cuisine is vulnerable to differences within a nation, because in the
   shared and embodied practices surrounding food, people make emotion-
   based evaluations of the nation-state. Uzbek governmental policies
   result in poverty, limited food production, little global food, and
   repression of pure Islamic practices, regions, and minority ethnic
   groups.},
