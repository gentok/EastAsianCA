@article{ WOS:000259620300008,
Author = {Knox, Colin},
Title = {Kazakhstan: modernizing government in the context of political inertia},
Journal = {INTERNATIONAL REVIEW OF ADMINISTRATIVE SCIENCES},
Year = {2008},
Volume = {74},
Number = {3},
Pages = {477-496},
Month = {SEP},
Abstract = {Kazakhstan declared its independence from the Soviet Union in 1991 and
   joined the Commonwealth of Independent States. Since then it has
   witnessed a remarkable economic transformation under the leadership of
   President Nursultan Nazarbayev. Pursuing a policy of `economy first and
   then politics', Kazakhstan is under growing pressure to engage in
