@incollection{ WOS:000412499000009,
Author = {Zholdoshalieva, Rakhat and Mun, Olga},
Editor = {Brown, EL and Zong, G},
Title = {ALIPPE, BUKVAR', AND GENDER A Comparative Analysis of Early Literacy
   Textbooks in Kazakhstan and Kyrgyzstan},
Booktitle = {GLOBAL PERSPECTIVES ON GENDER AND SEXUALITY IN EDUCATION: RAISING
   AWARENESS, FOSTERING EQUITY, ADVANCING JUSTICE},
Series = {International Advances in Education-Global Initiatives for Equity and
   Social Justice},
Year = {2017},
Pages = {147-168},
Abstract = {Critical theories on the relationship of education and the state argue
   that school curricular materials are means to shape ``official{''}
   knowledge and ideology through education (Apple, 1993). The existing
   literature on the former Soviet education argued that the official
   curricular materials, including school textbooks, intended to instill
   the Soviet communist ideology as ``the official knowledge,{''} shaped a
   certain version of ``truth{''} in the minds of young students, and
   controlled alternative interpretations and positions. The emerging
   literature in post-Soviet education documents the changes in early
   literacy textbooks such as bukvar (Russian name for primers) and exposes
   the process of ``official knowledge{''} production. Such studies reveal
   how childhood, motherland, and citizenship are being reimagined in the
   process of nation-state building in Latvia, Ukraine, and Armenia (Mead
   \& Silova, 2013; Silova, 1996). The existing studies on the ideological
   changes of citizenship, childhood, nation-state, and education do not
   include gender as another significant unit of analysis to fully
   understand how contemporary nationalist ideologies of citizenship and
   childhood shape certain perspectives of gender and identity.
   Acknowledging the critical role of textbooks in the process of national
   and gendered identity construction and positioning, this study addresses
   the existing gap in the literature. This study examines this issue by
   analyzing early literacy textbooks in Kazakhstan and Kyrgyzstan. We
   employ feminist critical discourse analysis framework (Fairclough, 2013;
   Lazar 2007; Sunderland \& Litosseliti, 2002) to investigate the
   relationship between ideology, literacy, ethnicity, and gender in 2
   early literacy textbooks in each country Alippe in Kazakh and Kyrgyz and
   Bukvar' in Russian. The multilevel analysis of textbook content, visual
   representation, and language, and the comparison within and across these
   countries reveal that the official knowledge is not only nationalist but
   it further promotes gendered ethnicity and citizenship, often
   demonstrating contradictions with what young people observe in their
   daily interactions and experiences.},
Publisher = {INFORMATION AGE PUBLISHING-IAP},
Address = {PO BOX 79049, CHARLOTTE, NC 28271-7047 USA},
