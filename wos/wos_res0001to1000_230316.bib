@article{ WOS:000457982600004,
Author = {Sartori, Paolo and Babajanov, Bakhtiyar},
Title = {Being Soviet, Muslim, Modernist, and Fundamentalist in 1950s Central
   Asia},
Journal = {JOURNAL OF THE ECONOMIC AND SOCIAL HISTORY OF THE ORIENT},
Year = {2019},
Volume = {62},
Number = {1},
Pages = {108-165},
Month = {DEC},
Abstract = {How far, if at all, did the intellectual legacy of early 20th-century
   Muslim reformism inform the transformative process which Islam underwent
   in Soviet Central Asia, especially after WWII? Little has been done so
   far to analyze the output of Muslim scholars ('ulama') operating under
   Soviet rule from the perspective of earlier Islamic intellectual
   traditions. The present essay addresses this problem and sheds light on
   manifestations of continuity among Islamic intellectual
   practices-mostly-puritanical-from the period immediately before the
   October Revolution to the 1950s. Such a continuity, we argue, profoundly
   informed the activity of the Spiritual Administration of the Muslims of
   Central Asia and Kazakhstan (SADUM) established in Tashkent in 1943 and,
   more specifically, the latter's attack against manifestations of
   religiosity deemed ``popular,{''} which were connected to the cult of
   saints. Thus, this essay posits that the juristic output of Soviet.
   `ulama' in Central Asia originates from and further develops an Islamic
   reformist thinking, which manifested itself in the region in the late
   19th- and early 20th-century. By establishing such an intellectual
   genealogy, we seek in this article to revise a historiographical
   narrative which has hitherto tended to decouple scripturalist
   sensibilities from Islamic reformism and modernism.},
