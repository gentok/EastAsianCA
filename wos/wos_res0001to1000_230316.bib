@article{ WOS:000712953600001,
Author = {Ziganshina, Dinara R. and de Schutter, Joop L. G.},
Title = {Paving the Way for Evidence-Driven Transboundary Water Cooperation in
   Central Asia},
Journal = {JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION},
Year = {2022},
Volume = {58},
Number = {6},
Pages = {1149-1161},
Month = {DEC},
Abstract = {This paper reviews the achievements and failures in pursuing
   evidence-driven and knowledge-based transboundary water cooperation in
   Central Asia, with a focus on scientific cooperation under the umbrella
