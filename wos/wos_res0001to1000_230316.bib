@article{ WOS:000263996700005,
Author = {Nourzhanov, Kirill},
Title = {Changing security threat perceptions in Central Asia},
Journal = {AUSTRALIAN JOURNAL OF INTERNATIONAL AFFAIRS},
Year = {2009},
Volume = {63},
Number = {1},
Pages = {85-104},
Abstract = {This article discusses the current process of securitisation in Central
   Asia and identifies its convoluted and faulty nature as a factor
   impeding collective security action in the region. It uses the Regional
   Security Complex Theory (RSCT) as an explanatory tool and posits that
   security discourse in and about the five former Soviet republics is
   dominated by geopolitical grand strategy on the one hand and by
   particularist concerns about lack of democracy or transnational threats
   on the other. Issues of conventional security involving two or more
   states, such as territorial disputes or resource management, are pushed
   aside and rarely securitised at the official level. The article outlines
   conceptual and institutional reasons for this bias, and argues that
   unless inter-state tensions are properly analysed, debated and
   addressed, the prospects for security and stability in the region will
   remain grim.},
