@article{ WOS:000320018800006,
Author = {Ruziev, Kobil and Majidov, Toshtemir},
Title = {Differing Effects of the Global Financial Crisis on the Central Asian
   Countries: Kazakhstan, the Kyrgyz Republic and Uzbekistan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2013},
Volume = {65},
Number = {4, SI},
Pages = {682-716},
Month = {JUN 1},
Abstract = {Kazakhstan, the Kyrgyz Republic and Uzbekistan are neighbouring
   countries in post-Soviet Central Asia which share similar culture and
   language. Their economic structures were similar under central planning:
   they provided the agricultural basis to the Soviet economy. But, since
   independence, these economies have grown structurally more heterogeneous
   due to variations in the implementation of market-oriented reforms, the
   degree of integration into the global economy and natural resource
   endowment. This article attempts to demonstrate how this heterogeneity
   can explain the differing effects of the recent Global Financial Crisis
   on these countries' economies in general and in the banking sector in
   particular.},
