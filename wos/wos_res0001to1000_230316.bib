@article{ WOS:000727503700004,
Author = {Arabsheibani, G. Reza and Kudebayeva, Alma and Mussurov, Altay},
Title = {A note on bride kidnapping and labour supply behaviour of Kyrgyz women},
Journal = {ECONOMIC SYSTEMS},
Year = {2021},
Volume = {45},
Number = {4},
Month = {DEC},
Abstract = {Using data from the 2011 and 2016 Life in Kyrgyzstan surveys, we examine
   Kyrgyz women's labour supply elasticities at the extensive margin. We
   use Heckman's two-step approach to predict earnings for the
   non-participating women and then use these predictions to estimate the
   participation equation. We find that women's labour supply decision is
   not influenced by their earnings. We also show that there exists a
   significant gap in employment propensities among ethnic Kyrgyz women in
   consensual or arranged marriages compared to women in kidnappedbased
   marriages. This finding suggests that the practice of bride abduction
   adversely affects women's probability of employment and might have
   negative consequences on their economic well-being.},
