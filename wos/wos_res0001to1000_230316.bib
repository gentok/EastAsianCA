@article{ WOS:000496291900004,
Author = {Mukhamedova, Nozilakhon and Pomfret, Richard},
Title = {Why Does Sharecropping Survive? Agrarian Institutions and Contract
   Choice in Kazakhstan and Uzbekistan},
Journal = {COMPARATIVE ECONOMIC STUDIES},
Year = {2019},
Volume = {61},
Number = {4},
Pages = {576-597},
Month = {DEC},
Abstract = {A century ago, Alfred Marshall demonstrated the inefficiency associated
   with farmers receiving only a portion of their marginal product. Farmers
   will supply less labor than under arrangements in which they receive
   their marginal product; output will be sub-optimal. Explanations of
   sharecropping are based on market imperfections, e.g., high transactions
   costs or inability to insure against risk, suggesting that sharecropping
   should disappear with economic development. Nevertheless, sharecropping
   survives. In Kazakhstan and Uzbekistan, sharecropping has no legal
   status but farm surveys provide evidence of its existence. Despite
   farmers' awareness of the Marshallian paradox, institutional uncertainty
   contributes to the persistent attractiveness of sharecropping.},
