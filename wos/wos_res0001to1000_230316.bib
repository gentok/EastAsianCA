@article{ WOS:000396681000005,
Author = {Pianciola, Niccolo},
Title = {Stalinist spatial hierarchies: placing the Kazakhs and Kyrgyz in Soviet
   economic regionalization},
Journal = {CENTRAL ASIAN SURVEY},
Year = {2017},
Volume = {36},
Number = {1},
Pages = {73-92},
Abstract = {Based on research in Russian and Kazakhstani archives, this article
   investigates connections between policies of peasant colonization, the
   sedentarization of pastoral nomadic peoples, and the economic
   regionalization of the USSR. After analysing debates from the 1920s, and
   limited sedentarization among Kazakhs and Kyrgyz during
   collectivization, the article argues that only by focusing on the
   economic regionalization of Central Asia, which placed the Kazakhs and
   the Kyrgyz in two different economic regions with dissimilar priorities,
   is it possible to explain the radically different outcomes of early
   Stalinist policies for similar pastoral peoples. The increased central
   control brought by the Stalinist Great Turn created a new spatial
   hierarchy directly connecting the bottom of the Soviet social and
   spatial pyramid, the livestock-breeding regions, to its top, the elite
   regime cities. The exclusion of the Kyrgyz ASSR from the massive
   livestock procurements that fed the Soviet political and industrial
   centres, and which led to the great famine in Kazakhstan (1931-33), can
   be explained by early Stalinist economic regionalization.},
