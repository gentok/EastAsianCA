@article{ WOS:000723469300001,
Author = {Dadabaev, Timur},
Title = {Nationhood through Neighborhood? From State Sovereignty to Regional
   Belonging in Central Asia},
Journal = {JOURNAL OF BORDERLANDS STUDIES},
Abstract = {This paper highlights the importance of reconsidering the meaning of the
   nationhood within artificially created borders in Central Asia in both
   theoretical and regional settings. This paper argues that there is a
   dichotomy in the images used to describe CA regional relations and
   nationhood in the IR discipline that either extensively relies on
   arguments along the lines of rivalry, domination, and spheres of
   influence or, alternatively, attempts to go beyond rationalist rhetoric
   by focusing on local understandings of various concepts and termswith
