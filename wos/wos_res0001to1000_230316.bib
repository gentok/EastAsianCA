@article{ WOS:000288374700004,
Author = {Tuncer-Kilavuz, Idil},
Title = {Understanding Civil War: A Comparison of Tajikistan and Uzbekistan},
Journal = {EUROPE-ASIA STUDIES},
Year = {2011},
Volume = {63},
Number = {2},
Pages = {263-290},
Abstract = {In order to explain why civil war erupted in Tajikistan but not in
   Uzbekistan this article assesses a range of theories that have sought to
   explain the occurrence of civil wars. After assessing the strengths and
   weaknesses of a number of these different existing approaches in the
   comparative literature, it argues that the eruption of civil war in
   Tajikistan and its absence in Uzbekistan can be explained by adapting
   ideas from bargaining theories of war that have been developed in the
   literature on international relations. It argues that it is important to
   study the adversaries' perceptions of power in relation to the actions
   that led to the civil war and how the perceptions of the distribution of
   power are influenced by structural, process and network-related
   variables. Consistent with bargaining theories of war, a disparity
   between the distribution of power and distribution of benefits seems an
   important factor in the eruption of civil war in Tajikistan.},
