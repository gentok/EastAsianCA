@article{ WOS:000541694200034,
Author = {Assanbayev, Mukhit B. and Hanks, Reuel R.},
Title = {KAZAKHSTAN'S RUSSIAN DIASPORA: BETWEEN MIGRATION AND SEARCH OF A NEW
   IDENTITY},
