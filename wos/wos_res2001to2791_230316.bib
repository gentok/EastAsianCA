@article{ WOS:000620522100001,
Author = {Harris-Smith, Kelsey and Palmer, Catherine},
Title = {Donor Funded Community-Based Tourism Development Projects: The
   Experience of Rural Mountain Communities in Tajikistan's Zerafshan
   Valley},
Journal = {TOURISM PLANNING \& DEVELOPMENT},
Year = {2022},
Volume = {19},
Number = {2},
Pages = {119-142},
Month = {MAR 4},
Abstract = {Research into the development of community-based tourism (CBT) within
   Central Asian economies that were part of the former USSR is
   underdeveloped. Even less understood is the role of government and
   international donor agencies in supporting CBT and the consequences for
   rural mountain communities; a gap addressed by this research focusing on
   the Republic of Tajikistan, specifically the Zerafshan valley and Fann
   mountains. In 2019, 20 qualitative interviews were conducted with a
   range of residents involved with three externally funded tourism
   development projects and/or with tourism. Project documents were
   analysed and the first author's extensive experience of living and
   working in Tajikistan also informed the research. The findings
   demonstrate that CBT has created jobs and generated income. However,
   benefits were not equally distributed, causing social tensions.
   Additionally, development projects did not foresee or account for
   structural limitations relating to infrastructure and accommodation. The
   article concludes with recommendations for future donor interventions.},
