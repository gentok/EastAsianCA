@article{ WOS:000771942700001,
Author = {Ahmadov, Baghish},
Title = {COMPARATIVE ASSESSMENT OF CAPITAL INCOME TAXATION IN AZERBAIJAN,
   KAZAKHSTAN, GEORGIA, AND BELARUS},
Journal = {JOURNAL OF EASTERN EUROPEAN AND CENTRAL ASIAN RESEARCH},
Year = {2022},
