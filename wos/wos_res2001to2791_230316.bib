ISSN = {0959-0552},
EISSN = {1758-6690},
Keywords = {Country-of-origin; Branding; Transitional country; Country image;
   Central Asia; Made in},
Keywords-Plus = {BRAND ORIGIN; CONSUMER; IMAGE; JAPANESE; PERCEPTIONS; DIMENSIONS;
