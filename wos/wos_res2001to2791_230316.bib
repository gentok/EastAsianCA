@article{ WOS:000485213600010,
Author = {Kuzhabekova, Aliya},
Title = {Performativity as cultural brokerage: An ethnographic exploration of the
   experiences of faculty mothers at an international university in
   Kazakhstan},
Journal = {WOMENS STUDIES INTERNATIONAL FORUM},
Year = {2019},
Volume = {75},
Month = {JUL-AUG},
Abstract = {In this ethnographic study, I explore the experiences of academic
   mothers who are international faculty in a highly internationalized
   residential university without tenure in Kazakhstan. Using participant
   observations, interviews, and auto-ethnography, I analyze how
   international female faculty perform their work and function within
   their family roles in the context of ambiguous overlapping cultural
   norms about gender roles and motherhood responsibilities. This study
   reveals that the participants could successfully combine and
   simultaneously enjoy their work and family lives. Moreover, these women
   could shape the emerging organizational culture of this relatively new
   higher educational institution in ways that are supportive of female
   faculty and student mothers. I use the cultural brokerage theory to
   explain the agentic capacity of female faculty and the way in which the
   theory may extend the previously used feminist performativity theory as
   a lens through which to investigate the transformative capacity of women
   on non-Western campuses.},
