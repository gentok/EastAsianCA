@article{ WOS:000581598000001,
Author = {Primiano, Christopher B. and Kudebayeva, Alma},
Title = {What is democracy and who supports it? Findings from a university survey
   in Kazakhstan},
Journal = {CENTRAL ASIAN SURVEY},
Year = {2020},
Volume = {39},
Number = {4},
Pages = {463-479},
Month = {OCT 1},
Abstract = {In recent years, many have said that democracy globally is being rolled
   back and that it is the source of much dissatisfaction. This article
   presents the findings from a survey of Kazakhstani university students
   on how they view democracy. The vast majority of participants view
   democracy as consisting of civil and political rights (i.e., the Western
   concept of democracy). However, also based on our survey results, our
   main argument is that, if forced to choose, they consider social
   services and economic development more important than political and
   civil rights. Another finding is that Muslims demonstrate stronger
   support for democracy than non-Muslims.},
