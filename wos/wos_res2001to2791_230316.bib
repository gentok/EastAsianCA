@article{ WOS:000455338100049,
Author = {Tokbolat, Serik and Karaca, Ferhat and Durdyev, Serdar and Nazipov,
   Farnush and Aidyngaliyev, Ilyas},
Title = {Assessment of Green Practices in Residential Buildings: A Survey-Based
