@article{ WOS:000396011600003,
Author = {Frank, Allen J.},
Title = {Shari'a Debates and Fatwas among Nomads in Northern Kazakhstan,
   1850-1931},
Journal = {ISLAMIC LAW AND SOCIETY},
Year = {2017},
Volume = {24},
Number = {1-2},
Pages = {61-76},
Abstract = {Legal debates among Kazakh nomads and on the Kazakh steppe more broadly
   have, for the most part, addressed the effects of Russian colonial
   policy on the administration of law among these nomads. The official and
   scholarly Russian fixation on Kazakh customary law, based largely on a
   tendentious categorization of Kazakh Muslims as quasi-shamanists,
   resulted in policies designed to separate Islamic law (shari'a) from
   customary law ('adat), and to suppress the role of shari'a in the areas
   of criminal and civil law. As Muslims, however, Kazakh nomads were
   directly affected by shari'a debates taking place both among Tatar
   scholars in their midst as well as among Kazakh scholars. These
   discussions, which occurred largely outside the field of vision of
   Russian officials or officially-mandated customary law courts, have so
   far eluded scrutiny. Recorded primarily in recently-published
   biographical dictionaries of Muslim scholars on the steppe, these
   discussions addressed a range of issues, including questions of ritual,
   but also, more significantly, the application of Islamic legal norms to
   commercial matters.},
