@article{ WOS:000786667000001,
Author = {Zainiddinov, Hakim},
Title = {Contextual Factors Associated With Women's Attitudes Toward Intimate
   Partner Violence in Tajikistan: Findings From the 2012 and 2017
   Demographic and Health Surveys},
Journal = {VIOLENCE AGAINST WOMEN},
Year = {2023},
Volume = {29},
Number = {2},
Pages = {112-133},
Month = {FEB},
Abstract = {The study employed data from the 2012 and 2017 Tajikistan Demographic
   and Health Surveys to examine two time periods on media access,
   interview setting, and sociodemographic predictors of intimate partner
   violence (IPV) justification attitudes among representative samples of
   women aged 15-49 years old (9,656 and 10,718 women, respectively) in
   Tajikistan. The odds of justifying IPV were higher for women who had
   access to radio and lower for those who had access to newspaper and
   television. The presence of the husband and other women during the
   interview was associated with lower justification of IPV. The findings
   emphasize the importance of contextual factors in developing effective
   IPV intervention policies.},
