@article{ WOS:000616662600004,
Author = {Kussaiynov, Talgat A. and Mussina, Gulnara S. and Bulkhairova, Zhanna S.
   and Saimagambetova, Gaukhar A.},
Title = {IMPACT OF EXPENSES ON HUMAN CAPITAL ON THE ECONOMIC GROWTH OF THE
   COUNTRY: CASE STUDY OF THE REPUBLIC OF KAZAKHSTAN},
Journal = {DETUROPE-THE CENTRAL EUROPEAN JOURNAL OF REGIONAL DEVELOPMENT AND
   TOURISM},
Year = {2020},
Volume = {12},
Number = {2},
Pages = {56-70},
Abstract = {Analysis of the impact of human capital on the efficiency of the economy
   of Kazakhstan was based on regression models with the use of statistical
   data on the volume of investments in professional and higher education,
