@article{ WOS:000832913200001,
Author = {Burashov, Shokan and Taitorina, Binur and Baisalova, Gulzira and
   Beissenbayeva, Manshuk and Azretbergenova, Indira},
Title = {Consumer Rights in the Republic of Kazakhstan and Their Legal
   Regulation: Transformation in the Context of Global Experience},
Journal = {PUBLIC INTEGRITY},
Abstract = {The relevance of improving consumer protection legislation is due to
   many economic and legal factors, starting with the conceptualization of
   common concepts. Against the background of recent political and legal
   initiatives in the field of consumer protection, this issue has become
   particularly important for the Republic of Kazakhstan (RK). In this
   study, a retrospective analysis of consumer protection regulation was
   carried out. Global trends in legal regulation of consumer protection in
   the RK are considered in the light of their implementation in
   Kazakhstani legislation. The research implies to consider the trends in
   changing the legislation of the Republic of Kazakhstan in the field of
   consumer rights regulation from the point of view of the reception of
   world experience. In the study, the analysis of consumer rights
   protection in the multifaceted system of legal protection models is
   carried out and their classification is derived. As a result of a
   comprehensive analysis, it was concluded that it is expedient to
   implement the norms of international law, European law into the national
   legislation of Kazakhstan, modernized in 2020, taking into account the
   proven examples of global legal mechanisms and practices for protecting
   consumer rights.},
